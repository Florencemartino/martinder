<h3>Voici la liste des amis disponibles</h3>
<ul>
  <% @users.each do |user| %>
    <li> <%= user.nickname %> </li>
    <% if user.photos.length > 0 %>
        <% user.photos.each do |photo| %>
          <%= cl_image_tag photo.key, height: 100, width: 100, crop: :fill %>
        <% end %>
    <% end %>
  <% end%>
</ul>
<div>
  <%= link_to "Retour", root_path, :class => "btn btn-primary" %>
</div>